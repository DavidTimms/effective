{
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typedoc": "^0.22.9",
    "typescript": "^4.4.4"
  },
  "name": "effective",
  "description": "An idiomatic IO monad for TypeScript",
  "version": "0.0.1",
  "main": "dist/io.js",
  "types": "dist/io.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --maxWorkers 2",
    "lint": "prettier --check 'src/**.ts' 'tests/**.ts'",
    "prepare": "npm run build",
    "generate-docs": "tsc --project docs-generator && typedoc --plugin ./docs-generator/dist/typedoc-effective-plugin.js src/io.ts",
    "prepublishOnly": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidTimms/effective.git"
  },
  "keywords": [
    "io",
    "monad",
    "pure",
    "functional",
    "concurrency"
  ],
  "author": "David Timms",
  "license": "UNLICENSED",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/DavidTimms/effective/issues"
  },
  "homepage": "https://github.com/DavidTimms/effective#readme",
  "dependencies": {
    "fast-check": "^2.14.0"
  }
}
